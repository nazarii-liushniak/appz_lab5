{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/train/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "trainservice",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/train/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/api/schedule/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "scheduleservice",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/schedule/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000"
  }
}
